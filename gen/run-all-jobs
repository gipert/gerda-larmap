#!/bin/bash
#
# Author: Luigi Pertoldi - pertoldi@pd.infn.it
# Created: Sun 24 Mar 2019

for i in `seq 0 9`; do
    [ ! -d out/00$i ]       && ./run-jobs-runid lar-vuv-complete.mac.template 100 00$i
    [ ! -d out/array-00$i ] && ./run-jobs-runid lar-vuv-array.mac.template 100 array-00$i
done

for i in `seq 10 99`; do
    [ ! -d out/0$i ] && ./run-jobs-runid lar-vuv-complete.mac.template 100 0$i
done

for i in `seq 100 300`; do
    [ ! -d out/$i ] && ./run-jobs-runid lar-vuv-complete.mac.template 100 $i
done
